{
  "name": "teamsync",
  "private": true,
  "homepage": "https://your-username.github.io/Teamsync",
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "start": "npm run electron-dev",
    "build": "vite build",
    "electron-dev": "cross-env ELECTRON_DEV=true npx electron .",
    "electron-build": "electron-builder",
    "lint": "eslint .",
